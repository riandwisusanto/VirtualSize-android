<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.virtualsize">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityRegister"></activity>
        <activity android:name=".ActivityDetail" />
        <activity android:name=".ActivityListProduk" />
        <activity android:name=".ActivityLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityUtama"
            android:screenOrientation="nosensor" />
        <activity android:name=".ActivityKontak" />
        <activity android:name=".ActivityProfil" />
        <activity android:name=".ActivityTentang" />
        <activity android:name=".ActivityUlasKami" />

    </application>
</manifest>